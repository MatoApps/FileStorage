{
  "ConnectionStrings": {
    "Default": "Server=localhost; Database=FileStorageDb; Trusted_Connection=True; TrustServerCertificate=True;"
  },
  "FileStorage": {
    "DirectorySeparator": "/",
    "RetainUnusedBlobs": false,
    "EnableAutoRename": true,
    "MaxByteSizeForEachFile": 1024,
    "MaxByteSizeForEachUpload": 4096,
    "MaxFileQuantityForEachUpload": 2,
    "AllowOnlyConfiguredFileExtensions": false,
    "FileExtensionsConfiguration": ".jpg,.png",
    "GetDownloadInfoTimesLimitEachUserPerMinute": 10
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
